<WpfControls:WPFWindow
    x:Class="DataBridge.GUI.MainWindow"
    Title="MainWindow" Width="800" Height="700"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:WpfControls="clr-namespace:DataBridge.GUI.Core.View.WPFControls"
    xmlns:UserControls="clr-namespace:DataBridge.GUI.UserControls"
    xmlns:utils="clr-namespace:DataBridge.GUI.Core.Utils"
    xmlns:Converters="clr-namespace:DataBridge.GUI.Converters"
    xmlns:Commands="clr-namespace:Microsoft.Practices.Prism.Commands;assembly=Microsoft.Practices.Prism"
    WindowStartupLocation="CenterScreen"
>
    <!--Background="#FFD1E0EF"-->
    <!--**************Resources******************-->
    <WpfControls:WPFWindow.Resources>
        <utils:DataContextProxy x:Key="DataContextProxy" />
        <Converters:DebugConverter x:Key="DebugConverter" />
    </WpfControls:WPFWindow.Resources>

    <Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="200*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="100*" />
        </Grid.RowDefinitions>
    </Grid>
</WpfControls:WPFWindow>